{
    "sudouest": {
        "matches": ["http://*.sudouest.fr/*"],
        "domain": ["sudouest.fr"],
        "selectors": ["#comments"]
    },
    "lemonde": {
        "matches": ["http://*.lemonde.fr/*"],
        "domain": ["lemonde.fr"],
        "selectors": [".reactions"]
    },
    "lefigaro": {
        "matches": ["http://*.lefigaro.fr/*"],
        "domain": ["lefigaro.fr"],
        "selectors": ["div.comment"]
    },
    "yahoo": {
        "matches": ["http://fr.news.yahoo.com/*"],
        "domain": ["fr.news.yahoo.com"],
        "selectors": [".yom-comments", "#yom-comments"]
    },
    "ladepeche": {
        "matches": ["http://*.ladepeche.fr/*"],
        "domain": ["ladepeche.fr"],
        "selectors": ["#commentaires"]
    },
    "lesechos": {
        "matches": ["http://*.lesechos.fr/*"],
        "domain": ["lesechos.fr"],
        "selectors": [".commentlist"]
    },
    "humanite": {
        "matches": ["http://*.humanite.fr/*"],
        "domain": ["humanite.fr"],
        "selectors": ["#comments", "#comment_box", ".comment_sep_number"]
    },
    "nouvelobs": {
        "matches": ["http://*.nouvelobs.com/*"],
        "domain": ["nouvelobs.com"],
        "selectors": ["#reactions"]
    },
    "lepoint": {
        "matches": ["http://*.lepoint.fr/*"],
        "domain": ["lepoint.fr"],
        "selectors": ["#bloc_commentaire", ".fermeture_commentaires"]
    },
    "leparisien": {
        "matches": ["http://*.leparisien.fr/*"],
        "domain": ["leparisien.fr"],
        "selectors": ["#blocreactions"]
    },
    "lequipe": {
        "matches": ["http://*.lequipe.fr/*"],
        "domain": ["lequipe.fr"],
        "selectors": ["#commentaires"]
    },
    "francefootball": {
        "matches": ["http://*.francefootball.fr/*"],
        "domain": ["francefootball.fr"],
        "selectors": [".commentaires"]
    }
}